
numbers = [12, 34, 45, 23, 43, 1, 5, 8, 4, 3]
desired_sum = 57

def indeces(numbers, desired_sum):
    for i in range(len(numbers)):
        for j in range(i + 1, len(numbers)):
            if (numbers[i] + numbers[j] == desired_sum):
               print(i, j)

indeces(numbers, desired_sum)







